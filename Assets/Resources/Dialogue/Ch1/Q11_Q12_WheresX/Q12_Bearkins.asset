    segments:
    - speakerName: Mayor Bearkins
      dialogueText: Ah, Dot! Good morning!
      speakerIcon: {fileID: 21300000, guid: 5d839bf8eef66c54c84ae09a7d0fc19d, type: 3}
      speakerBkg: {fileID: 21300000, guid: d2978a09ef6a0824b904da80573c2906, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Mayor Bearkins
      dialogueText: You look quite frantic! Is everything alright?
      speakerIcon: {fileID: 21300000, guid: 5d839bf8eef66c54c84ae09a7d0fc19d, type: 3}
      speakerBkg: {fileID: 21300000, guid: d2978a09ef6a0824b904da80573c2906, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: Have you seen [NAME] around?
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Mayor Bearkins
      dialogueText: '[NAME]? Who are you talking about?'
      speakerIcon: {fileID: 21300000, guid: 5d839bf8eef66c54c84ae09a7d0fc19d, type: 3}
      speakerBkg: {fileID: 21300000, guid: d2978a09ef6a0824b904da80573c2906, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: ...
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: '[NAME]! Your niece?!'
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Mayor Bearkins
      dialogueText: Excuse me? I do <b>not</b> have a niece.
      speakerIcon: {fileID: 21300000, guid: 5d839bf8eef66c54c84ae09a7d0fc19d, type: 3}
      speakerBkg: {fileID: 21300000, guid: d2978a09ef6a0824b904da80573c2906, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: What do you mean?! [NAME] is your niece! You mentioned her multiple
        times yesterday!
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Mayor Bearkins
      dialogueText: "Young one, I haven\u2019t the <i>slightest</i> idea who you
        are talking about!"
      speakerIcon: {fileID: 21300000, guid: 5d839bf8eef66c54c84ae09a7d0fc19d, type: 3}
      speakerBkg: {fileID: 21300000, guid: d2978a09ef6a0824b904da80573c2906, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: I uh...
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: Okay well, I went to meet Roscoe yesterday like you said!
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: And we found... <i>something</i>.
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Mayor Bearkins
      dialogueText: Something? What kind of something, what on earth did you find?
      speakerIcon: {fileID: 21300000, guid: 5d839bf8eef66c54c84ae09a7d0fc19d, type: 3}
      speakerBkg: {fileID: 21300000, guid: d2978a09ef6a0824b904da80573c2906, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: "I can\u2019t really explain what it is-"
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: Wait! I have some proof!
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
