  RootNode:
    choiceText: 
    segments:
    - speakerName: Sally
      dialogueText: Oh! Hello!
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: 0
      choices: []
    - speakerName: Dot
      dialogueText: "Hello! I\u2019m Dot! What\u2019s your name?"
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: "My name\u2019s Sally! I\u2019ve never seen you before- Are you
        new here?"
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: "I am! I just got here from\u2026 um.."
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: "I was at the beach! I saw the Captain and he told me that there\u2019s
        a town here."
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: "Well, you\u2019re here! Welcome to town!"
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: You should go see my uncle, the Mayor! He can tell you all about
        the town. You can find him further down the path in the Town Hall.
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: ...But first, can you do something for me?
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: Sure! What do you need?
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: Do you think you can give this letter to that guy in the store?
        I kinda like him, and-
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: Um... well... ykno...
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Dot
      dialogueText: Absolutely! Anything you want me to tell him?
      speakerIcon: {fileID: 21300000, guid: 9088c58d64497cd43bdbdf146db7c8a2, type: 3}
      speakerBkg: {fileID: 21300000, guid: a65be46bd8f25ea45887454e2b147ec3, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: "Ummm.. no! Everything\u2019s there in the letter!"
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: -1
      choices: []
    - speakerName: Sally
      dialogueText: Thank you, thank you so much!
      speakerIcon: {fileID: 21300000, guid: e4226d3a0e51f90439f682009363b95f, type: 3}
      speakerBkg: {fileID: 21300000, guid: 24578ba7d438cb14fbc62126740e80e8, type: 3}
      nextBranch: {fileID: 0}
      actionIndex: 1
      choices: []
